local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source.lua'))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Key system", HidePremium = false, SaveConfig = true, IntroEnabled = false})

OrionLib:MakeNotification({
    Name = "You're logged",
    Content = "You are logged in as " .. Player.Name,
    Image = "rbxassetid://4483345998",
    Time = 5
})

_G.key = "balkan69#"
_G.keyInput = ""

function MakeScripthub()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Balkanx/Testing/main/Testing.lua", true))()
end

function CorrectrrKeyNotification()
    OrionLib:MakeNotification({
        Name = "Key link!",
        Content = "Key link copied!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Correct key!",
        Content = "You have entered the correct key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
    OrionLib:Destroy()
end

function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Incorrect key!",
        Content = "You have entered the incorrect key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local Tab = Window:MakeTab({
    Name = "Key",
    PremiumOnly = false
})

Tab:AddTextbox({
    Name = "Enter key",
    Default = "",
    TextDisappear = true,
    Callback = function(Value)
        _G.keyInput = Value
    end
})

Tab:AddButton({
    Name = "Check key",
    Callback = function()
        if _G.keyInput == _G.key then
            MakeScripthub()
            CorrectKeyNotification()
        else
            IncorrectKeyNotification()
        end
    end
})

Tab:AddButton({
    Name = "Key link",
    Callback = function()
        setclipboard("https://discord.com/invite/z2QcdakrJK")
        print("Join our server! https://discord.com/invite/z2QcdakrJK")
        CorrectrrKeyNotification()
    end
})

OrionLib:Init()
